{
  "name": "nuxt-ioc",
  "version": "1.0.0",
  "description": "Nuxt IOC implementation",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "test": "jest",
    "build": "rimraf lib && webpack",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json"
  },
  "files": [
    "lib/index.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mateuszgachowski/nuxt-ioc.git"
  },
  "keywords": [
    "ioc",
    "nuxt",
    "inversion",
    "di"
  ],
  "author": "Mateusz Gachowski",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mateuszgachowski/nuxt-ioc/issues"
  },
  "homepage": "https://github.com/mateuszgachowski/nuxt-ioc#readme",
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.0",
    "inversify": "^5.0.1",
    "jest": "26.4.2",
    "lodash": "^4.17.20",
    "prettier": "2.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "ts-jest": "26.2.0",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "typescript": "4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "@types/inversify": "^2.0.33",
    "vue": "^2.6.12"
  }
}
